{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "no-multi-spaces": 0,
        "no-unused-vars": "off"
    }
}